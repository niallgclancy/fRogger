PRECIP = train$PRECIP
BFI = train$BFI
model = occ~slope+TEMP+ELEV+CANOPY+PRECIP+BFI
frog.rf = randomForest(model)
eval.rf = eval(frog.rf)
eval.rf
varImpPlot(frog.rf)
